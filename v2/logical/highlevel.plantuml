package EGF {

}

' object Error 
class TrustRegistry {
    EGFURI
}

class Query {
    AuthorizedUnderRole(entityDID,role)
    RecognizedRegistry(registryDID)
    GetResource(resourceDID)  
}

class Lookup {
    DIDMethods() 
    Roles()
    ResourceTypes()
    ' CredentialTypes()
    ' CredentialSchemas()
    ' PresentationRequests()
    ' Overlays()
    AssuranceLevels()
}

class Metadata {
    TrustRegistryName: string
    RawAPIEndpoint: URL 
    DiscoveryAPI: URL 
    Profiles()
